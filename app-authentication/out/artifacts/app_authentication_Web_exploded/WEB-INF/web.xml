<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>sign_up</servlet-name>
        <servlet-class>uz.pdp.controller.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign_up</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>uz.pdp.controller.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>sign_up2</servlet-name>
        <servlet-class>uz.test.controller.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sign_up2</servlet-name>
        <url-pattern>/register2</url-pattern>
    </servlet-mapping>
</web-app>